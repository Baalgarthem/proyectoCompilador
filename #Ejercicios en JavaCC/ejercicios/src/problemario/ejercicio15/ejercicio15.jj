options{  static = true;}PARSER_BEGIN(ejercicio15)package problemario.ejercicio15;import java.io.*;import java.util.*;public class ejercicio15{  public static void main(String args []) throws ParseException  {    ejercicio15 parser = new ejercicio15(new StringReader("h5221"));    Token token = parser.getNextToken();    if (token.kind == ejercicio15.CADENA_ACEPTADA)    {      System.out.println("Lexema aceptado: " + token.image);    }    else    {      System.out.println("Lexema DENEGADO...");    }  }}PARSER_END(ejercicio15)SKIP :{  " "| "\r"| "\t"| "\n"}TOKEN :{  < CADENA_ACEPTADA: ((< LETRA > < DIGITO >) (<VEINTIUNO >) ) >}TOKEN : //primero defino cada token que se ocupar√°{  < DIGITO : ([ "0"-"9" ])+ >| <VEINTIUNO:"12" > | < LETRA : ([ "a"-"z", "A"-"Z" ])* > //cualesquiera numero de letras, puede que ninguna}