options{  static = true;}PARSER_BEGIN(ejercicio3)package problemario.ejercicio3;
import java.io.*;
public class ejercicio3{  public static void main(String args []) throws ParseException  { 	ejercicio3 parser = new ejercicio3(new StringReader("<"));    Token token = parser.getNextToken();    if (token.kind == ejercicio3.CADENA_ACEPTADA)    {      System.out.println("Cadena ACEPTADA: " + token.image);    }    else {		System.out.println("Cadena no aceptada...");      }  }}PARSER_END(ejercicio3)SKIP :{  " "| "\r"| "\t"| "\n"}TOKEN: {  < CADENA_ACEPTADA: (< MAYOR > )|(< MENOR > )|(< MAYORIGUAL > )|(< MENORIGUAL > ) >   }TOKEN: {< MENOR: "<" >| < MAYOR: ">" >| < MENORIGUAL: "<=" >| < MAYORIGUAL:">=" >  }
