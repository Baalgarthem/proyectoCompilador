options{  static = true;}PARSER_BEGIN(ejercicio4)package problemarioRegex.ejercicio04;
import java.io.*;
public class ejercicio4{  public static void main(String args []) throws ParseException  {	ejercicio4 leer = new ejercicio4(new StringReader("596432mzlkam"));    Token token = leer.getNextToken();        if (token.kind == ejercicio4.CADENACOMBINADA)    {      System.out.println("Esta cadena es aceptada: " + token.image);    }    else {		System.out.println("La cadena ha sido rechazada");      }  }}PARSER_END(ejercicio4)SKIP :{  " "| "\r"| "\t"| "\n"}TOKEN: {<CADENACOMBINADA: (< DIGITO >)+ (< DIGITO >)*(<NUMEROPAR >)(< VOCALES >) >  }TOKEN: { <DIGITO: (["0"-"9"]) >| < NUMEROPAR: ("0"| "2"| "4"| "6"| "8")>|  < VOCALES: (["a"]| ["e"] | ["i"] | ["o"] | ["u"] ) >  }  